<p id="notice"><%= notice %></p>

<h1>Assigments Cost Centers</h1>

<table>
  <thead>
    <tr>
      <th>Cost center</th>
      <th>Assignated</th>
      <th>Operator</th>
      <th>Client</th>
      <th>Active</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Assignation status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @assigments_cost_centers.each do |assigments_cost_center| %>
      <tr>
        <td><%= assigments_cost_center.cost_center %></td>
        <td><%= assigments_cost_center.assignated %></td>
        <td><%= assigments_cost_center.operator %></td>
        <td><%= assigments_cost_center.client %></td>
        <td><%= assigments_cost_center.active %></td>
        <td><%= assigments_cost_center.start_date %></td>
        <td><%= assigments_cost_center.end_date %></td>
        <td><%= assigments_cost_center.assignation_status %></td>
        <td><%= link_to 'Show', assigments_cost_center %></td>
        <td><%= link_to 'Edit', edit_assigments_cost_center_path(assigments_cost_center) %></td>
        <td><%= link_to 'Destroy', assigments_cost_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assigments Cost Center', new_assigments_cost_center_path %>
