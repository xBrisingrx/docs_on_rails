<div class="modal-dialog modal-lg" role="document">
  <div class="modal-content">
    <div class="modal-header g-brd-blue g-bg-blue">
      <h5 class="modal-title text-white"><strong><%= @title_modal%></strong></h5>
      <button type="button" class="close text-white" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
      <% document_renovation.file.each do |renovation| %>
				<div class="g-py-20 g-px-10">
					<%= link_to '<i class="fa fa-file-pdf-o fa-2x"></i>'.html_safe, renovation, target: '_blank' %>
				</div>
			<% end %>
    </div>
  </div>
</div>